{
    "name": "artarts36/swagger-merger",
    "description": "Swagger Merger",
    "type": "library",
    "require": {
        "symfony/yaml": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^9.5",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ArtARTs36",
            "email": "temicska99@mail.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArtARTs36\\SwaggerMerger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtARTs36\\SwaggerMerger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": [
            "./vendor/bin/phpcs --standard=PSR2 src/"
        ],
        "test": [
            "./vendor/bin/phpunit tests"
        ]
    }
}
