paths:
    /users:
        post:
            summary: 'Adds a new user'
            requestBody:
                content:
                    application/json:
                        schema:
                            type: object
                            properties:
                                id:
                                    type: integer
                                name:
                                    type: string
                            example:
                                id: 10
                                name: 'Jessica Smith'
            responses:
                201:
                    description: OK
        get:
            summary: 'get users'
            responses:
                200:
                    description: OK
components: {  }
